{
	"name": "mrim.pl",
	"version": "1.0.0",
	"description": "Personal website",
	"scripts": {
		"dev": "webpack-dev-server --inline --progress --config webpack/dev.js",
		"build": "webpack --inline --progress --config webpack/dist.js"
	},
	"author": "Maciej Rim",
	"license": "GPL-3.0",
	"browserslist": [
		"defaults"
	],
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"modules": false
				}
			],
			"@babel/preset-react"
		],
		"plugins": [
			"@babel/plugin-syntax-dynamic-import"
		]
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"env": {
			"browser": true,
			"es6": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"globals": {
			"Atomics": "readonly",
			"SharedArrayBuffer": "readonly"
		},
		"parserOptions": {
			"ecmaVersion": 2019,
			"sourceType": "module"
		},
		"plugins": [
			"react"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.4",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"browserslist": "^4.12.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.19.0",
		"file-loader": "^6.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-loader": "^1.1.0",
		"html-webpack-plugin": "^4.2.0",
		"prop-types": "^15.7.2",
		"terser-webpack-plugin": "^2.3.5",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	},
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"jss": "^10.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-ga": "^2.7.0",
		"react-jss": "^10.1.1"
	}
}
