{
	"name": "mrim.pl",
	"version": "1.0.0",
	"description": "Personal website",
	"scripts": {
		"dev": "webpack-dev-server --inline --progress --config webpack/dev.js",
		"build": "webpack --inline --progress --config webpack/dist.js"
	},
	"author": "Maciej Rim",
	"license": "GPL-3.0",
	"browserslist": [
		"defaults"
	],
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"modules": false
				}
			],
			"@babel/preset-react"
		],
		"plugins": [
			"@babel/plugin-syntax-dynamic-import"
		]
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"env": {
			"browser": true,
			"es6": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"globals": {
			"Atomics": "readonly",
			"SharedArrayBuffer": "readonly"
		},
		"parserOptions": {
			"ecmaVersion": 2019,
			"sourceType": "module"
		},
		"plugins": [
			"react"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.6.2",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"browserslist": "^4.7.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"eslint": "^6.5.1",
		"eslint-plugin-react": "^7.16.0",
		"file-loader": "^4.2.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.8.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"sass": "^1.23.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"terser-webpack-plugin": "^2.1.2",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "^3.8.2",
		"webpack-merge": "^4.2.2"
	},
	"dependencies": {
		"@babel/runtime": "^7.6.2",
		"date-fns": "^2.4.1",
		"react": "^16.10.2",
		"react-dom": "^16.10.2"
	}
}
